{
  "expo": {
    "name": "Lazada Clone",
    "slug": "rn-ecommerce",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/laz-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/laz-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rnlazada.clone"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/laz-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/laz-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4a097352-87ef-4cf4-917d-d7028db058bb"
      }
    },
    "owner": "adrian9502"
  }
}
